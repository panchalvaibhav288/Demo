import java.util.Scanner;
class Swapping{
    public static void main(String [],args){
        Scanner sc=new Scanner(System.in);
        int a,b;
        System.out.println("Enter two no.");
        a=sc.nextInt();
        b=sc.nextInt();
        a=a*b;
        b=a/b;
        a=a/b;
        System.out.println("A=>"+a+"B=>"+b);
    }
}
